    $j(document).ready(function() {
        var test = $j('div').hasClass("subcat-directory") || !$j('nav').hasClass("block-layered-nav");
        if($j('div').hasClass("subcat-directory") || !$j('nav').hasClass("block-layered-nav")) {
            $j('.category-view > .category-info').attr('style','width: 100%');
        }
    });

    AddShoppersTracking = {
       header: "",
       image: "",
       url: "https://www.infowarsstore.com/weekly-deals.html?___store=default&amp;p=2",
       name: "Home Page: Weekly Deals",
